{
  "name": "smapp",
  "version": "0.0.1",
  "private": true,
  "description": "Spacemesh Coin Wallet Desktop Application",
  "scripts": {
    "build": "npm run build-main && npm run build-renderer",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
    "build-main": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.renderer.prod.babel.js --colors",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./configs/checkPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "flow": "flow",
    "flow-typed": "rm -rf flow-typed && flow-typed install",
    "lint": "cross-env NODE_ENV=development eslint --format=pretty .",
    "lint-ci": "npm run flow && npm run lint",
    "package": "npm run build && electron-builder build --publish never",
    "package-all": "npm run build && electron-builder build -mwl",
    "package-ci": "npm run build && electron-builder --publish always",
    "postinstall": "npm run flow-typed && electron-builder install-app-deps package.json && npm run build-dll",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./desktop/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./desktop/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js"
  },
  "main": "./desktop/main.prod.js",
  "build": {
    "productName": "SmApp Wallet",
    "appId": "smapp",
    "files": [
      "desktop/dist/",
      "desktop/app.html",
      "desktop/main.prod.js",
      "desktop/main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github",
      "owner": "smapp",
      "repo": "smapp",
      "private": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacemeshos/smapp.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spacemeshos/smapp/issues"
  },
  "keywords": [
    "Apps",
    "Seed",
    "app",
    "cosmic",
    "spacemeshos",
    "smapp"
  ],
  "homepage": "https://github.com/spacemeshos/smapp#readme",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-proposal-do-expressions": "^7.2.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.3.2",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@grpc/proto-loader": "^0.4.0",
    "aes-js": "^3.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-root-slash-import": "^1.1.0",
    "cross-env": "^5.2.0",
    "detect-port": "^1.3.0",
    "electron": "4.0.3",
    "electron-builder": "^20.39.0",
    "electron-debug": "^2.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-log": "^3.0.1",
    "electron-updater": "^4.0.6",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "exports-loader": "^0.7.0",
    "file-loader": "^3.0.1",
    "flow-bin": "^0.93.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "imports-loader": "^0.8.0",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "terser-webpack-plugin": "^1.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.4",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "bip39": "^2.5.0",
    "grpc": "^1.18.0",
    "lodash.get": "^4.4.2",
    "pbkdf2": "^3.0.17",
    "qrcode.react": "^0.9.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.5",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.6.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "source-map-support": "^0.5.10",
    "styled-components": "^4.1.3",
    "tweetnacl": "^1.0.1",
    "uuid4": "^1.1.4"
  },
  "browserslist": "electron 1.6"
}
