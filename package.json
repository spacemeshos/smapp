{
  "name": "smapp",
  "productName": "Spacemesh",
  "version": "0.0.1",
  "author": {
    "name": "Spacemech",
    "email": "info@spacemesh.io",
    "url": "https://spacemesh.io/"
  },
  "private": true,
  "description": "Spacemesh Coin Wallet Desktop Application",
  "scripts": {
    "build": "npm run build-main && npm run build-renderer",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
    "build-main": "cross-env NODE_ENV=production cross-env DEBUG_PROD=true webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production cross-env DEBUG_PROD=true webpack --config ./configs/webpack.config.renderer.prod.babel.js --colors",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./configs/checkPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "flow": "flow",
    "flow-typed": "rm -rf flow-typed && flow-typed install",
    "lint": "cross-env NODE_ENV=development eslint --format=pretty .",
    "lint-ci": "npm run flow && npm run lint",
    "package-all": "npm run build && node ./scripts/packagerScript.js --target mwl",
    "package-mac": "npm run build && node ./scripts/packagerScript.js --target mac",
    "package-win": "npm run build && node ./scripts/packagerScript.js --target windows",
    "package-linux": "npm run build && node ./scripts/packagerScript.js --target linux",
    "package-ci": "npm run build && node ./scripts/packagerScript.js --target mwl --publish always",
    "package-mac-ci": "npm run build && node ./scripts/packagerScript.js --target mac --publish always",
    "postinstall": "npm run flow-typed && electron-builder install-app-deps && npm run build-dll",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./desktop/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./desktop/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js"
  },
  "main": "./desktop/main.prod.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spacemeshos/smapp.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spacemeshos/smapp/issues"
  },
  "keywords": [
    "Apps",
    "Seed",
    "app",
    "cosmic",
    "spacemeshos",
    "smapp"
  ],
  "homepage": "https://github.com/spacemeshos/smapp#readme",
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-export-default-from": "7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "7.7.4",
    "@babel/plugin-proposal-optional-chaining": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-syntax-import-meta": "7.7.4",
    "@babel/plugin-transform-react-constant-elements": "7.7.4",
    "@babel/plugin-transform-react-inline-elements": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-flow": "7.7.4",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.4",
    "@hot-loader/react-dom": "16.11.0",
    "aes-js": "3.1.2",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-dev-expression": "0.2.2",
    "babel-plugin-styled-components": "1.10.6",
    "babel-root-slash-import": "1.1.0",
    "cross-env": "5.2.0",
    "detect-port": "1.3.0",
    "dotenv": "8.2.0",
    "electron": "7.0.1",
    "electron-builder": "22.1.0",
    "electron-debug": "3.0.1",
    "electron-devtools-installer": "2.2.4",
    "electron-notarize": "0.2.0",
    "electron-unhandled": "^3.0.2",
    "electron-updater": "4.2.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
